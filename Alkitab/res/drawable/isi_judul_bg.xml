<?xml version="1.0" encoding="UTF-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item
		android:state_pressed="true">
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient
				android:angle="270"
				android:startColor="#464646"
				android:endColor="#404040"
				android:type="linear" />
		</shape>
	</item>

	<item>
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:shape="rectangle">
			<gradient
				android:angle="270"
				android:startColor="#9b9b9b"
				android:endColor="#808080"
				android:type="linear" />
		</shape>
	</item>
</selector>