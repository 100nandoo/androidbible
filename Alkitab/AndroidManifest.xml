<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="yuku.alkitab"
	android:installLocation="auto"
	android:versionCode="32"
	android:versionName="1.1-dev1">
	<application
		android:name="yuku.alkitab.base.AlkitabApplication"
		android:icon="@drawable/icon"
		android:label="@string/app_name"
		>
		<activity
			android:name="yuku.alkitab.base.IsiActivity"
			android:label="Alkitab"
			android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation">
			<intent-filter>
				<action
					android:name="android.intent.action.MAIN" />
				<category
					android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="yuku.alkitab.base.EdisiActivity"
			android:label="Edisi" />
		<activity
			android:name="yuku.alkitab.base.MenujuActivity"
			android:label="Menuju" />

		<activity
			android:name="yuku.alkitab.base.PengaturanActivity"
			android:label="Pengaturan Alkitab" />
		<activity
			android:name="yuku.alkitab.base.BukmakActivity"
			android:label="Pembatas Buku" />

		<activity
			android:name="yuku.alkitab.base.Search2Activity"
			android:label="Pencarian"
			android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation" />
		<activity
			android:name="yuku.alkitab.base.RenunganActivity"
			android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation" />
		<activity
			android:name="yuku.alkitab.base.BantuanActivity"
			android:label="Bantuan" />
	</application>

	<uses-sdk
		android:minSdkVersion="3"
		android:targetSdkVersion="4" />

	<uses-permission
		android:name="android.permission.INTERNET" />
	<uses-permission
		android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

	<supports-screens />
</manifest> 